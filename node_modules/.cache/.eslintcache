\n    data = JSON.parse(data)\n    data.push(찾은상품.id)\n    data = new Set(data) // Set(중복 제거 배열)로 변환\n    data = Array.from(data) // arr로 다시 변환\n    localStorage.setItem('watched', JSON.stringify(data))\n  }, [])\n\n  useEffect(() => {\n    let timer = setTimeout(() => {\n      setAlert(false);\n    }, 2000);\n\n    // useeffect보다 먼저 실행\n    return () => {\n      // 기존 타이머 제거\n      clearTimeout(timer);\n    };\n  }, []);\n\n  let [num, setNum] = useState(\"\");\n\n  useEffect(() => {\n    if (isNaN(num) == true) {\n      alert(\"그러지 마세요\");\n    }\n  }, [num]);\n\n  let state = useSelector((state) => {return state})\n  let dispatch = useDispatch()\n\n  return (\n    <div className=\"container\">\n      {/* <YellowBtn bg=\"blue\">버튼</YellowBtn>\n      <YellowBtn bg=\"orange\">버튼</YellowBtn> */}\n\n      {alert == true ? <div className=\"alert alert-warning\">2초 이내 구매시 할인</div> : null}\n\n      <div className=\"row\">\n        <div className=\"col-md-6\">\n          <img src=\"https://codingapple1.github.io/shop/shoes1.jpg\" width=\"100%\" />\n        </div>\n        <div className=\"col-md-6\">\n          <input onChange={(e) => {setNum(e.target.value)}} />\n          <h4 className=\"pt-5\">{찾은상품.title}</h4>\n          <p>{찾은상품.content}</p>\n          <p>{찾은상품.price}원</p>\n          <button className=\"btn btn-danger\" onClick={() => {\n              dispatch(addItem({ id: 1, name: \"ㅇㅇㅇ\", count: 1 }))\n          }}>주문하기</button>\n        </div>\n      </div>\n\n      <Link to='/cart'>장바구니</Link>\n\n    <Nav variant=\"tabs\" defaultActiveKey=\"/home\">\n      <Nav.Item>\n        <Nav.Link href=\"/home\" onClick={() => { setTab(0)}}>버튼0</Nav.Link>\n      </Nav.Item>\n      <Nav.Item>\n        <Nav.Link eventKey=\"link-1\" onClick={() => { setTab(1)}}>버튼1</Nav.Link>\n      </Nav.Item>\n      <Nav.Item>\n        <Nav.Link eventKey=\"link-2\" onClick={() => { setTab(2)}}>버튼2</Nav.Link>\n      </Nav.Item>\n    </Nav>\n    <TabContent tab={tab}/>\n\n\n    </div>\n  );\n}\n\nfunction TabContent({tab}) {\n  // if ( props.tab == 0) {\n  //   return <div>내용0</div>\n  // } else if ( props.tab == 1) {\n  //   return <div>내용1</div>\n  // } else if ( props.tab == 2) {\n  //   return <div>내용2</div>\n  // }\n\n  let [fade, setFade ] = useState('')\n  let {재고, shoes} = useContext(Context1) // porps 없이 사용가능...\n\n\n  useEffect(()=> {\n    // timer도 걸어줘야 함, automatic batching 기능 때문에,,, : state 변경이 모두 된 후에 재랜더링..\n    let timer = setTimeout(() => {setFade('end')}, 100)\n\n    // css 부착 위해..\n    return () => {\n      cleanup(timer)\n      setFade('')\n    }\n  }, [tab])\n\n  return (<div className={`start ${fade}`}> {\n    [<div>내용0</div>, <div>내용1</div>, <div>내용2</div>][tab]\n  }</div>\n  )\n}\n\n","/Users/jang-uijin/Desktop/코딩/코딩애플/shop/src/routes/Cart.js",["63","64"],[],"/Users/jang-uijin/Desktop/코딩/코딩애플/shop/src/store.js",[],[],"/Users/jang-uijin/Desktop/코딩/코딩애플/shop/src/store/userSlice.js",[],[],{"ruleId":"65","severity":1,"message":"66","line":3,"column":10,"nodeType":"67","messageId":"68","endLine":3,"endColumn":16},{"ruleId":"65","severity":1,"message":"69","line":5,"column":25,"nodeType":"67","messageId":"68","endLine":5,"endColumn":29},{"ruleId":"65","severity":1,"message":"70","line":28,"column":12,"nodeType":"67","messageId":"68","endLine":28,"endColumn":16},{"ruleId":"71","severity":1,"message":"72","line":33,"column":34,"nodeType":"73","messageId":"74","endLine":33,"endColumn":35},{"ruleId":"75","severity":1,"message":"76","line":127,"column":9,"nodeType":"77","endLine":127,"endColumn":103},{"ruleId":"65","severity":1,"message":"78","line":2,"column":10,"nodeType":"67","messageId":"68","endLine":2,"endColumn":15},{"ruleId":"65","severity":1,"message":"79","line":11,"column":5,"nodeType":"67","messageId":"68","endLine":11,"endColumn":14},{"ruleId":"65","severity":1,"message":"80","line":18,"column":5,"nodeType":"67","messageId":"68","endLine":18,"endColumn":8},{"ruleId":"65","severity":1,"message":"81","li